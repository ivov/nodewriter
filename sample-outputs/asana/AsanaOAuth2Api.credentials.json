{
	"name": "asanaOAuth2Api",
	"displayName": "Asana OAuth2 API",
	"documentationUrl": "https://docs.n8n.io/integrations/creating-nodes/",
	"extends": ["oAuth2Api"],
	"properties": [
		{
			"displayName": "Grant Type",
			"name": "grantType",
			"type": "hidden",
			"default": "authorizationCode"
		},
		{
			"displayName": "Authorization URL",
			"name": "authUrl",
			"type": "hidden",
			"default": "https://app.asana.com/-/oauth_authorize",
			"required": true
		},
		{
			"displayName": "Access Token URL",
			"name": "accessTokenUrl",
			"type": "hidden",
			"default": "https://app.asana.com/-/oauth_token",
			"required": true
		},
		{
			"displayName": "Scope",
			"name": "scope",
			"type": "hidden",
			"default": "default email openid profile"
		}
	]
}